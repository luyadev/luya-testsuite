{
    "name" : "luyadev/luya-testsuite",
    "description" : "TestCases and Data for LUYA Modules and Components. Makes testing less pain.",
    "type" : "luya-module",
    "keywords" : ["php", "luya", "module"],
    "license": "MIT",
    "homepage" : "http://luya.io",
    "authors" : [
        {
            "name" : "Basil Suter",
            "email" : "git@nadar.io",
            "homepage" : "https://github.com/nadar"
        }
    ],
    "require" : {
        "phpunit/phpunit" : "~6.0.0",
        "curl/curl" : "^1.0",
        "twig/twig" : "^1.0",
        "luyadev/luya-core" : "~1.0.0",
        "luyadev/luya-module-cms" : "~1.0.0",
        "luyadev/luya-module-admin" : "~1.0.0"
    },
    "support" : {
        "issues" : "https://github.com/luyadev/luya/issues"
    },
    "autoload" : {
        "psr-4" : {
            "luya\\testsuite\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "luya\\testsuite\\tests\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
